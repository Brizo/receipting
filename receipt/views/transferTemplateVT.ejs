<div class='row-fluid'  ng-show="choseTransferVT" id="transferTemplate">                                       
    <div class="form-group">
      <label for="bankCode" class="col-sm-4 control-label">Bank Code:</label>
      <div class="col-sm-6">
      	<select class="form-control" id="bankCode" ng-disabled= "isMultiPay" ng-model="bankCodeVT">
        <option>--Select--</option>
        <option>FNB</option>
        <option>NED</option>
        <option>STD</option>
        <option>SWB</option>
        <option>SBS</option>
        </select>
      </div>
    </div>
    
    <div class="form-group">
      <label for="drawerName" class="col-sm-4 control-label">Drawer's Name:</label>
      <div class="col-sm-6">
        <input type="text" class="form-control" id="drawerName" placeholder="Drawer's Name" ng-disabled= "isMultiPay" ng-model="drawerNameVT">
      </div>
    </div>

</div>